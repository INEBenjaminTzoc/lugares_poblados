  A2          Decimal? @db.Decimal(23, 0)
  C2          Decimal? @db.Decimal(23, 0)
  A3          Decimal? @db.Decimal(23, 0)
  C3          Decimal? @db.Decimal(23, 0)
  A4          Decimal? @db.Decimal(23, 0)
  C4          Decimal? @db.Decimal(23, 0)
